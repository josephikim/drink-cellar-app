{
  "compilerOptions": {
    "target": "ES5",
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "strictNullChecks": true,
    "declaration": false,
    "noEmit": true,
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "useUnknownInCatchVariables": false
  }
}
